def checkarmstrong(n):
    x=n
    t=n
    c=0
    s=0
    while x>0:
        d=x%10
        c=c+1
        x=x//10
    while t>0:
        r=t%10
        s=s+r**c
        t=t//10
    if(s==n):
        return True

n=int(input())
is_armstrong =checkarmstrong(n)
if is_armstrong:
    print("true")
else:
    print("false")
    
output - 
371, as 3^3 + 7^3 + 1^3 = 371
1634, as 1^4 + 6^4 + 3^4 + 4^4 = 1634
