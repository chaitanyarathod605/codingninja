def fibonacci(n):
    if num==0 or num==1 or num==2:
        return True
    num1=0
    num2=1
    while num2<num:
        series=num1+num2
        num1=num2
        num2=series
    if num2==num:
        return True
    return False
num=int(input())
if(fibonacci(num)):
    print("true")
else:
    print("false")
    
    
  output - 
  F(n) = F(n-1) + F(n-2)
