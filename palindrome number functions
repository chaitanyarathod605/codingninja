def checkPalindrome(num):
    rev = 0
    n = num
    while (n > 0):
        r = n % 10
        rev = (rev * 10) + r
        n = n // 10
    if (rev == num):
        return True
    else:
        return False

num = int(input())
isPalindrome = checkPalindrome(num)
if (isPalindrome):
    print('true')
else:
    print('false')
    
  output - 
  121
