n=int(input())
if (n==0):
    print(1)
elif(n==1):
        print(1)
else:
     print(1)
     print(11)

     i=3
     while i<=n:
        j=1
        while j<=2:
            print(1, end="")
            if(i>=3 and j==1):
                t=j
                while t<(i-1):
                    print(0, end="")
                    t=t+1
            j=j+1
        print()
        i=i+1
        
